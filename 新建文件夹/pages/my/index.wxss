page,.wrap{
  width:100%;
  height: 100%;
  background: rgba(0,0,0,.05);
}
.nav{
  display: flex;
  align-items: center;
  background: #fff;
  padding:20rpx;
  border-bottom: 1rpx solid #ccc;
}
.img{
  width:150rpx;
  height: 150rpx;
  border-radius: 50%;
  margin: 20rpx 30rpx 20rpx 0
}
.right{
  flex: 1;
  display: flex;
  justify-content: space-between
}
.fen{
  color: orange
}
.cont{
  margin-top:60rpx;
}
.icon{
  width:70rpx;
  height: 70rpx;
  border-radius: 50%;
  margin: 20rpx 30rpx 20rpx 0;
}
.list{
  display: flex;
  align-items: center;
  background: #FFF;
  border-bottom: 1rpx solid #ccc;
  padding: 0 20rpx;
}
.aa{
  display: flex;
  flex: 1;
  justify-content: space-between
}